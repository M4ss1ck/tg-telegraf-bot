{
  "name": "telegraf-bot",
  "version": "1.0.0",
  "description": "Telegram bot using telegraf",
  "main": "./dist/main.js",
  "scripts": {
    "start": "tsc -p ./tsconfig.json && node -r dotenv/config ./dist/main.js",
    "build": "tsc -p ./tsconfig.json",
    "validate": "prisma validate",
    "db-pull": "prisma db pull",
    "generate": "prisma generate",
    "seed": "prisma generate && prisma db push && prisma db seed",
    "studio": "prisma studio",
    "watch": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node -r dotenv/config ./dist/main.js\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "M4ss1ck",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "axios": "^0.25.0",
    "cheerio": "^1.0.0-rc.10",
    "dotenv": "^10.0.0",
    "expr-eval": "^2.0.2",
    "fast-csv": "^4.3.6",
    "pg": "^8.7.3",
    "telegraf": "^4.8.1",
    "twig": "^1.15.4",
    "urban-dictionary-client": "^3.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@types/node": "^18.7.18",
    "eslint": "^8.23.1",
    "nodemon": "^2.0.19",
    "prisma": "^4.3.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.3"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
